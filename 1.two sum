class Solution {
    public int[] twoSum(int[] nums, int target) {
        int i=0;
        int j=0;
        int[] res=new int[2];
        for(;i<nums.length-1;i++)
            for(j=i+1;j<=nums.length-1;j++)
                if(nums[i]+nums[j]==target)
                {    res[0]=i;
                     res[1]=j;
                     return(res);
                };
asdasdasda
    }
}
